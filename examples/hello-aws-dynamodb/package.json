{
  "name": "hello-aws-dynamodb",
  "description": "Example of a service using AWS DynamoDB.",
  "version": "0.0.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22.7"
  },
  "workspaces": [
    "packages/*",
