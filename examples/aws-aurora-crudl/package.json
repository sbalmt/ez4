{
  "name": "aws-aurora-crudl",
  "description": "Example of a service using AWS Aurora for CRUDL.",
  "version": "0.0.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22.7"
  },
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "seed": "tsc && ez4 run -e local.env -- ./src/seed.ts",
    "deploy": "tsc && ez4 deploy -e local.env",
    "destroy": "ez4 destroy -e local.env --force",
    "generate:erd": "ez4 generate -- database:erd docs/",
    "serve": "ez4 serve -e local.env"
  },
  "dependencies": {
    "@ez4/aws-aurora": "^0.33.0",
    "@ez4/aws-gateway": "^0.33.0",
    "@ez4/common": "^0.33.0",
    "@ez4/database": "^0.33.0",
    "@ez4/docs-database": "^0.33.0",
    "@ez4/gateway": "^0.33.0",
    "@ez4/local-gateway": "^0.33.0",
    "@ez4/local-database": "^0.33.0",
    "@ez4/project": "^0.33.0",
    "@ez4/schema": "^0.33.0"
  }
}