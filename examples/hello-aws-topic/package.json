{
  "name": "hello-aws-topic",
  "description": "Example of a service using AWS topics (SNS).",
  "version": "0.0.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22.7"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "exports": {
    ".": {
      "types": "./src/export.ts",
      "default": "./src/export.js"
    }
  },
  "scripts": {
    "deploy": "tsc -p tsconfig.json && ez4 deploy -e local.env -p ez4.project.js",
    "destroy": "ez4 destroy -e local.env -p ez4.project.js",
    "serve": "ez4 serve -e local.env -p ez4.project.js --debug",
    "test": "ez4 test -e local.env -p ez4.project.js"
  },
  "dependencies": {
    "@ez4/aws-common": "^0.26.0",
    "@ez4/aws-topic": "^0.26.0",
    "@ez4/aws-queue": "^0.26.0",
    "@ez4/common": "^0.26.0",
    "@ez4/local-topic": "^0.26.0",
    "@ez4/local-queue": "^0.26.0",
    "@ez4/topic": "^0.26.0",
    "@ez4/queue": "^0.26.0",
    "@ez4/project": "^0.26.0"
  }
}