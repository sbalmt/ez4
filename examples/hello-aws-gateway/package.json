{
  "name": "hello-aws-gateway",
  "description": "Example of a service using AWS API Gateway.",
  "version": "0.0.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22.7"
  },
  "workspaces": [
    "examples/*"
  ],
  "exports": {
    ".": "./src/export.ts"
  },
  "scripts": {
    "deploy": "tsc && ez4 deploy -e local.env",
    "destroy": "ez4 destroy -e local.env",
    "generate:oas": "ez4 generate -e local.env -- gateway:oas docs/",
    "serve": "ez4 serve -e local.env",
    "output": "ez4 output -e local.env"
  },
  "dependencies": {
    "@ez4/aws-gateway": "^0.37.0",
    "@ez4/common": "^0.37.0",
    "@ez4/docs-gateway": "^0.37.0",
    "@ez4/local-gateway": "^0.37.0",
    "@ez4/gateway": "^0.37.0",
    "@ez4/project": "^0.37.0",
    "@ez4/schema": "^0.37.0"
  }
}