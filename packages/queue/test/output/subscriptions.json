{
  "TestQueue": {
    "type": "@ez4/queue",
    "name": "TestQueue",
    "description": "Queue to test subscriptions.",
    "schema": {
      "type": "object",
      "properties": {
        "foo": {
          "type": "string"
        }
      }
    },
    "subscriptions": [
      {
        "handler": {
          "name": "testHandler",
          "file": "test/input/output-subscriptions.ts"
        }
      },
      {
        "handler": {
          "name": "testHandler",
          "file": "test/input/output-subscriptions.ts"
        },
        "memory": 128,
        "variables": {
          "TEST_VAR": "test-literal-value"
        }
      }
    ],
    "services": {
      "selfClient": "TestQueue"
    }
  }
}