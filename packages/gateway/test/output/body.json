        "handler": {
          "name": "testRouteC",
          "file": "test/input/output-body.ts",
          "request": {
            "body": {
              "type": "object",
              "description": "Expected HTTP body payload.",
              "identity": 1,
              "properties": {
                "fooKey": {
                  "type": "string"
