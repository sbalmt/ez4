{
  "TestService": {
    "type": "@ez4/http",
    "name": "TestService",
    "routes": [
      {
        "path": "GET /test-route",
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-body.ts",
          "request": {
            "body": {
              "type": "object",
              "description": "Expected JSON body payload.",
              "properties": {
                "foo": {
                  "type": "string"
                },
                "bar": {
                  "type": "number"
                }
              }
            }
          },
          "response": {
            "type": "object",
            "properties": {
              "status": {
                "type": "number",
                "description": "Response status code.",
                "extra": {
                  "value": 204
                }
              }
            }
          }
        }
      }
    ]
  }
}