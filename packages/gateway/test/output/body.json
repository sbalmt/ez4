      {
        "path": "GET /test-route-a",
        "handler": {
          "name": "testRouteA",
          "file": "test/input/output-body.ts",
          "request": {
            "body": {
              "type": "string",
              "description": "Expected HTTP body payload."
            }
          },
          "response": {
            "status": 204
          }
        }
      },
      {
        "path": "GET /test-route-b",
        "handler": {
