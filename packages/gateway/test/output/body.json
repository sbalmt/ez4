{
  "TestService": {
    "type": "@ez4/http",
    "description": "Service for testing query strings.",
    "id": "ez4-test-service",
    "name": "Test Service",
    "routes": [
      {
        "path": "GET /test-route",
        "handler": {
          "name": "testRoute",
          "file": "test/input/body.ts",
          "request": {
            "body": {
              "type": "object",
              "properties": {
                "foo": {
                  "type": "string"
                },
                "bar": {
                  "type": "number"
                }
              },
              "description": "Expected field for body payload."
            }
          },
          "response": {
            "type": "object",
            "properties": {
              "status": {
                "type": "number",
                "description": "Response status code."
              }
            }
          }
        }
      }
    ]
  }
}