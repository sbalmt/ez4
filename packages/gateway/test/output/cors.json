{
  "TestService": {
    "type": "@ez4/http",
    "name": "TestService",
    "routes": [
      {
        "path": "POST /test-route-1",
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-cors.ts",
          "response": {
            "status": 204
          }
        }
      },
      {
        "path": "PATCH /test-route-2",
        "cors": true,
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-cors.ts",
          "response": {
            "status": 204
          }
        }
      }
    ],
    "cors": {
      "allowOrigins": [
        "*"
      ],
      "allowMethods": [
        "*"
      ],
      "allowCredentials": true,
      "allowHeaders": [
        "x-income-header"
      ],
      "exposeHeaders": [
        "x-exposed-header"
      ],
      "maxAge": 300
    }
  }
}