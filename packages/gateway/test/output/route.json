{
  "TestService": {
    "type": "@ez4/http",
    "id": "TestService",
    "description": "Service for testing routes.",
    "routes": [
      {
        "path": "ANY /test-route-1",
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-route.ts",
          "response": {
            "type": "object",
            "properties": {
              "status": {
                "type": "number",
                "description": "Response status code."
              }
            }
          }
        }
      },
      {
        "path": "GET /test-route-2",
        "timeout": 30,
        "memory": 512,
        "variables": {
          "TEST_VAR": "test-literal-value"
        },
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-route.ts",
          "response": {
            "type": "object",
            "properties": {
              "status": {
                "type": "number",
                "description": "Response status code."
              }
            }
          }
        }
      }
    ]
  }
}