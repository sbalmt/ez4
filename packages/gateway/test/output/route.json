{
  "TestService": {
    "type": "@ez4/http",
    "name": "TestService",
    "routes": [
      {
        "path": "ANY /test-route-1",
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-route.ts",
          "response": {
            "status": 204
          }
        }
      },
      {
        "path": "GET /test-route-2",
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-route.ts",
          "response": {
            "status": 204
          }
        },
        "timeout": 30,
        "memory": 512,
        "variables": {
          "TEST_VAR": "test-literal-value"
        }
      }
    ]
  }
}