{
  "name": "@ez4/docs-topology",
  "description": "EZ4: Topology documentation generator",
  "version": "0.38.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "ez4",
    "generator",
    "documentation",
    "topology"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbalmt/ez4.git",
    "directory": "providers/docs/docs-topology"
  },
  "engines": {
    "node": ">=22.7"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/main.cjs",
      "import": "./dist/main.mjs"
    }
  },
  "workspaces": [
    "foundation/*",
    "providers/docs/*"
  ],
  "scripts": {
    "lint": "eslint --cache",
    "clean": "rm -f *.tsbuildinfo && rm -rf dist/*",
    "build": "tsc && node tools/bundler.mjs",
    "test": "npm run build && tsc -p tsconfig.test.json && ez4 test",
    "local:publish": "npm run build && npm run clean:registry && npm publish --registry http://localhost:4873",
    "clean:registry": "rm -rf ../../../.registry/@ez4/docs-topology",
    "live:publish": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@ez4/common": "^0.38.0",
    "@ez4/distribution": "^0.38.0",
    "@ez4/database": "^0.38.0",
    "@ez4/factory": "^0.38.0",
    "@ez4/queue": "^0.38.0",
    "@ez4/gateway": "^0.38.0",
    "@ez4/project": "^0.38.0",
    "@ez4/scheduler": "^0.38.0",
    "@ez4/schema": "^0.38.0",
    "@ez4/storage": "^0.38.0",
    "@ez4/topic": "^0.38.0",
    "@ez4/utils": "^0.38.0",
    "@ez4/validation": "^0.38.0"
  }
}