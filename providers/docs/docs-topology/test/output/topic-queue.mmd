%%{init: { "layout": "elk", "theme": "light" } }%%
graph LR
	classDef ez4Queue fill:#2dbd6e50, stroke:#2dbd6ea0;
	classDef ez4ImportQueue fill:#2dbd6e25, stroke:#2dbd6ea0, stroke-dasharray:5 5;;
	classDef ez4QueueEdge stroke:#2dbd6e
	subgraph Queues
		style Queues fill: #f5f5f5, stroke-width: 0
		TestQueue@{ shape: das, label: "TestQueue" }
		class TestQueue ez4Queue
	end
	classDef ez4Topic fill:#f2c94c50, stroke:#f2c94ca0;
	classDef ez4ImportTopic fill:#f2c94c25, stroke:#f2c94ca0, stroke-dasharray:5 5;;
	classDef ez4TopicEdge stroke:#f2c94c;
	subgraph Topics
		style Topics fill: #f5f5f5, stroke-width: 0
		TestTopic@{ shape: das, label: "TestTopic" }
		class TestTopic ez4Topic
	end
	TestTopic TestQueueTestTopic@--> TestQueue
	class TestQueueTestTopic ez4TopicEdge