{
    "name": "@ez4/docs-gateway-ui",
    "description": "EZ4: Gateway documentation UI",
    "version": "0.35.0",
    "author": "Wellington Martins",
    "license": "MIT",
    "type": "module",
    "keywords": [
        "ez4",
        "documentation",
        "gateway",
        "ui"
    ],
    "engines": {
        "node": ">=22.7"
    },
    "exports": {
        ".": {
            "types": "./dist/main.d.ts",
            "require": "./dist/main.cjs",
            "import": "./dist/main.mjs"
        }
    },
    "scripts": {
        "lint": "eslint --cache",
        "clean": "rm -f *.tsbuildinfo && rm -rf dist/*",
        "build": "tsc && node tools/bundler.mjs",
        "test": "npm run build && tsc -p tsconfig.test.json && ez4 test",
        "local:publish": "npm run build && npm run clean:registry && npm publish --registry http://localhost:4873",
        "clean:registry": "rm -rf ../../../.registry/@ez4/docs-gateway-ui",
        "live:publish": "npm run build && npm publish --access public"
    },
    "dependencies": {
        "@ez4/aws-function": "^0.35.0",
        "@ez4/aws-gateway": "^0.35.0",
        "@ez4/gateway": "^0.35.0",
        "@ez4/project": "^0.35.0",
        "@ez4/schema": "^0.35.0",
        "@ez4/utils": "^0.35.0",
        "yaml": "^2.8.2"
    }
}