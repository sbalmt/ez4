{
  "name": "@ez4/aws-common",
  "description": "EZ4: Common components for self-managed AWS providers",
  "version": "0.29.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "ez4"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbalmt/ez4.git",
    "directory": "providers/aws/aws-common"
  },
  "engines": {
    "node": ">=22.7"
  },
  "exports": {
    "types": "./dist/main.d.ts",
    "require": "./dist/main.cjs",
    "import": "./dist/main.mjs"
  },
  "workspaces": [
    "foundation/*",
    "contracts/*",
    "providers/aws/*"
  ],
  "scripts": {
    "lint": "eslint --cache",
    "clean": "rm -f *.tsbuildinfo && rm -rf dist/*",
    "build": "tsc && node tools/bundler.mjs",
    "test": "npm run build && tsc -p tsconfig.test.json && ez4 test -e ../../../local.env",
    "local:publish": "npm run build && npm run clean:registry && npm publish --registry http://localhost:4873",
    "clean:registry": "rm -rf ../../../.registry/@ez4/aws-common",
    "live:publish": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/client-sts": "^3.758.0",
    "@ez4/project": "^0.29.0",
    "@ez4/reflection": "^0.29.0",
    "@ez4/stateful": "^0.29.0",
    "@ez4/utils": "^0.29.0",
    "esbuild": "^0.25.9"
  }
}
