{
  "name": "@ez4/aws-function",
  "description": "EZ4: Self-managed AWS lambda function provider",
  "version": "0.40.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "ez4"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbalmt/ez4.git",
    "directory": "providers/aws/aws-function"
  },
  "engines": {
    "node": ">=22.7"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/main.cjs",
      "import": "./dist/main.mjs"
    }
  },
  "sideEffects": [
    "./dist/main.cjs",
    "./dist/main.mjs"
  ],
  "workspaces": [
    "foundation/*",
    "contracts/*",
    "providers/aws/*"
  ],
  "scripts": {
    "lint": "eslint --cache",
    "clean": "rm -f *.tsbuildinfo && rm -rf dist/*",
    "build": "tsc && node tools/bundler.mjs",
    "test": "npm run build && tsc -p tsconfig.test.json && ez4 test -e ../../../local.env",
    "local:publish": "npm run build && npm run clean:registry && npm publish --registry http://localhost:4873",
    "clean:registry": "rm -rf ../../../.registry/@ez4/aws-function",
    "live:publish": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.958.0",
    "@ez4/aws-common": "^0.40.0",
    "@ez4/aws-identity": "^0.40.0",
    "@ez4/aws-logs": "^0.40.0",
    "@ez4/project": "^0.40.0",
    "@ez4/common": "^0.40.0",
    "@ez4/stateful": "^0.40.0",
    "@ez4/utils": "^0.40.0",
    "@types/adm-zip": "^0.5.5",
    "adm-zip": "^0.5.14"
  }
}