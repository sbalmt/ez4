{
  "name": "@ez4/aws-email",
  "description": "EZ4: Self-managed AWS Simple Email Service provider",
  "version": "0.43.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "ez4"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbalmt/ez4.git",
    "directory": "providers/aws/aws-email"
  },
  "engines": {
    "node": ">=22.7"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/main.cjs",
      "import": "./dist/main.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "require": "./dist/client.cjs",
      "import": "./dist/client.mjs"
    }
  },
  "sideEffects": [
    "./dist/main.cjs",
    "./dist/main.mjs"
  ],
  "workspaces": [
    "foundation/*",
    "contracts/*",
    "providers/aws/*"
  ],
  "scripts": {
    "lint": "eslint --cache",
    "clean": "rm -rf dist/* *.tsbuildinfo .eslintcache",
    "build": "tsc && node tools/bundler.mjs",
    "test": "npm run build && tsc -p tsconfig.test.json && ez4 test -e ../../../local.env",
    "local:publish": "npm run build && npm run clean:registry && npm publish --registry http://localhost:4873",
    "clean:registry": "rm -rf ../../../.registry/@ez4/aws-email",
    "live:publish": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.996.0",
    "@ez4/aws-common": "^0.43.0",
    "@ez4/aws-identity": "^0.43.0",
    "@ez4/common": "^0.43.0",
    "@ez4/email": "^0.43.0",
    "@ez4/project": "^0.43.0",
    "@ez4/stateful": "^0.43.0",
    "@ez4/utils": "^0.43.0"
  }
}