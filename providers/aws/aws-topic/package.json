{
  "name": "@ez4/aws-topic",
  "description": "EZ4: Self-managed AWS SNS topics provider",
  "version": "0.30.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "ez4"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbalmt/ez4.git",
    "directory": "providers/aws/aws-topic"
  },
  "engines": {
    "node": ">=22.7"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/main.cjs",
      "import": "./dist/main.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "require": "./dist/client.cjs",
      "import": "./dist/client.mjs"
    }
  },
  "sideEffects": [
    "./dist/main.cjs",
    "./dist/main.mjs"
  ],
  "workspaces": [
    "foundation/*",
    "contracts/*",
    "providers/aws/*"
  ],
  "scripts": {
    "lint": "eslint --cache",
    "clean": "rm -f *.tsbuildinfo && rm -rf dist/*",
    "build": "tsc && node tools/bundler.mjs",
    "test": "npm run build && tsc -p tsconfig.test.json && ez4 test -e ../../../local.env",
    "local:publish": "npm run build && npm run clean:registry && npm publish --registry http://localhost:4873",
    "clean:registry": "rm -rf ../../../.registry/@ez4/aws-topic",
    "live:publish": "npm run build && npm publish --access public"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.156"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.913.0",
    "@ez4/aws-common": "^0.30.0",
    "@ez4/aws-identity": "^0.30.0",
    "@ez4/aws-function": "^0.30.0",
    "@ez4/aws-logs": "^0.30.0",
    "@ez4/aws-queue": "^0.30.0",
    "@ez4/topic": "^0.30.0",
    "@ez4/project": "^0.30.0",
    "@ez4/queue": "^0.30.0",
    "@ez4/schema": "^0.30.0",
    "@ez4/stateful": "^0.30.0",
    "@ez4/transform": "^0.30.0",
    "@ez4/validator": "^0.30.0",
    "@ez4/utils": "^0.30.0"
  }
}