{
  "name": "@ez4/aws-aurora",
  "description": "EZ4: Self-managed AWS Aurora serverless v2 provider",
  "version": "0.26.0",
  "author": "Silas B.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "ez4"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbalmt/ez4.git",
    "directory": "providers/aws/aws-aurora"
  },
  "engines": {
    "node": ">=22.7"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/main.cjs",
      "import": "./dist/main.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "require": "./dist/client.cjs",
      "import": "./dist/client.mjs"
    }
  },
  "workspaces": [
    "foundation/*",
    "contracts/*",
    "providers/aws/*"
  ],
  "scripts": {
    "lint": "eslint --cache",
    "clean": "rm -f *.tsbuildinfo && rm -rf dist/*",
    "build": "tsc && node tools/bundler.mjs",
    "test": "npm run build && ez4 test -e ../../../local.env",
    "local:publish": "npm run build && npm run clean:registry && npm publish --registry http://localhost:4873",
    "clean:registry": "rm -rf ../../../.registry/@ez4/aws-aurora",
    "live:publish": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@aws-sdk/client-rds": "^3.758.0",
    "@aws-sdk/client-rds-data": "^3.758.0",
    "@ez4/aws-common": "^0.26.0",
    "@ez4/aws-identity": "^0.26.0",
    "@ez4/database": "^0.26.0",
    "@ez4/pgclient": "^0.26.0",
    "@ez4/pgmigration": "^0.26.0",
    "@ez4/pgsql": "^0.26.0",
    "@ez4/project": "^0.26.0",
    "@ez4/stateful": "^0.26.0",
    "@ez4/schema": "^0.26.0",
    "@ez4/utils": "^0.26.0"
  }
}