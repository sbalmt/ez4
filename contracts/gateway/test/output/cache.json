{
  "@variables": {
    "name": "@variables",
    "type": "@ez4/common",
    "context": {}
  },
  "TestService": {
    "type": "@ez4/http",
    "context": {},
    "name": "TestService",
    "routes": [
      {
        "path": "POST /test-route-1",
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-cache.ts",
          "response": {
            "status": 204
          }
        }
      },
      {
        "path": "PATCH /test-route-2",
        "authorizer": {
          "name": "testAuthorizer",
          "file": "test/input/output-cache.ts",
          "response": {
            "identity": {
              "identity": 1,
              "type": "object",
              "description": "Authorization identity.",
              "properties": {}
            }
          }
        },
        "handler": {
          "name": "testRoute",
          "file": "test/input/output-cache.ts",
          "response": {
            "status": 204
          }
        }
      }
    ],
    "cache": {
      "authorizerTTL": 5
    }
  }
}