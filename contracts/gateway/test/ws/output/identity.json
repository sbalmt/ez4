{
  "@variables": {
    "type": "@ez4/common",
    "name": "@variables",
    "context": {}
  },
  "TestService": {
    "type": "@ez4/ws",
    "context": {},
    "name": "TestService",
    "schema": {
      "type": "object",
      "identity": 1,
      "properties": {
        "foo": {
          "type": "string"
        },
        "bar": {
          "type": "number"
        }
      }
    },
    "routeKey": "foo",
    "connect": {
      "handler": {
        "name": "connectHandler",
        "file": "test/ws/input/output-identity.ts",
        "request": {
          "identity": {
            "type": "object",
            "description": "Expected identity.",
            "identity": 1,
            "properties": {
              "id": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "disconnect": {
      "handler": {
        "name": "disconnectHandler",
        "file": "test/ws/input/output-identity.ts"
      }
    },
    "message": {
      "handler": {
        "name": "messageHandler",
        "file": "test/ws/input/output-identity.ts",
        "request": {
          "body": {
            "type": "object",
            "description": "Expected event body.",
            "identity": 1,
            "properties": {
              "foo": {
                "type": "string"
              },
              "bar": {
                "type": "number"
              }
            }
          },
          "identity": {
            "type": "object",
            "description": "Expected identity.",
            "identity": 1,
            "properties": {
              "id": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}