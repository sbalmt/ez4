{
  "@variables": {
    "name": "@variables",
    "type": "@ez4/common",
    "context": {}
  },
  "TestService": {
    "type": "@ez4/http",
    "context": {},
    "variables": {
      "TEST_VAR1": "test-literal-value",
      "TEST_VAR2": "test-env-var-value"
    },
    "services": {
      "selfSettings": "@variables"
    },
    "name": "TestService",
    "routes": [
      {
        "path": "ANY /test-route",
        "variables": {
          "TEST_VAR3": "test-literal-route-value"
        },
        "handler": {
          "name": "testRoute",
          "file": "test/http/input/output-variables.ts",
          "request": {},
          "provider": {
            "services": {
              "selfSettings": "@variables"
            },
            "variables": {
              "TEST_VAR1": "test-literal-value",
              "TEST_VAR2": "test-env-var-value"
            }
          },
          "response": {
            "status": 204
          }
        }
      }
    ]
  }
}