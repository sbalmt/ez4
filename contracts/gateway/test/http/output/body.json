{
  "@variables": {
    "name": "@variables",
    "type": "@ez4/common",
    "context": {}
  },
  "TestService": {
    "type": "@ez4/http",
    "name": "TestService",
    "context": {},
    "routes": [
      {
        "path": "GET /test-route-a",
        "handler": {
          "name": "testRouteA",
          "file": "test/http/input/output-body.ts",
          "request": {
            "body": {
              "type": "string",
              "description": "Expected HTTP body payload."
            }
          },
          "response": {
            "status": 204
          }
        }
      },
      {
        "path": "GET /test-route-b",
        "handler": {
          "name": "testRouteB",
          "file": "test/http/input/output-body.ts",
          "request": {
            "body": {
              "type": "object",
              "description": "Expected HTTP body payload.",
              "identity": 1,
              "properties": {
                "foo": {
                  "type": "string"
                },
                "bar": {
                  "type": "number"
                }
              }
            }
          },
          "response": {
            "status": 204
          }
        }
      },
      {
        "path": "GET /test-route-c",
        "handler": {
          "name": "testRouteC",
          "file": "test/http/input/output-body.ts",
          "request": {
            "body": {
              "type": "union",
              "description": "Expected HTTP body payload.",
              "optional": true,
              "elements": [
                {
                  "type": "object",
                  "identity": 1,
                  "properties": {
                    "foo": {
                      "type": "string"
                    }
                  }
                }
              ]
            }
          },
          "response": {
            "status": 204
          }
        }
      },
      {
        "path": "GET /test-route-d",
        "preferences": {
          "namingStyle": "snake"
        },
        "handler": {
          "name": "testRouteD",
          "file": "test/http/input/output-body.ts",
          "request": {
            "body": {
              "type": "object",
              "description": "Expected HTTP body payload.",
              "identity": 1,
              "properties": {
                "fooKey": {
                  "type": "string"
                },
                "barKey": {
                  "type": "number"
                }
              }
            }
          },
          "response": {
            "status": 204
          }
        }
      }
    ]
  }
}