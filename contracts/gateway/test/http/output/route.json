{
  "@variables": {
    "name": "@variables",
    "type": "@ez4/common",
    "context": {}
  },
  "TestService": {
    "type": "@ez4/http",
    "name": "TestService",
    "context": {},
    "variables": {},
    "services": {},
    "routes": [
      {
        "path": "ANY /test-route-1",
        "logRetention": 7,
        "disabled": true,
        "handler": {
          "name": "testRoute1",
          "file": "test/http/input/output-route.ts",
          "response": {
            "status": 204
          }
        }
      },
      {
        "path": "GET /test-route-2",
        "handler": {
          "name": "testRoute2",
          "file": "test/http/input/output-route.ts",
          "response": {
            "status": 204
          }
        },
        "timeout": 30,
        "memory": 512,
        "variables": {
          "TEST_VAR": "test-literal-value"
        }
      }
    ]
  }
}