{
  "@variables": {
    "type": "@ez4/common",
    "name": "@variables",
    "context": {}
  },
  "TestTopic": {
    "type": "@ez4/topic",
    "name": "TestTopic",
    "context": {},
    "variables": {},
    "services": {
      "selfClient": "TestTopic"
    },
    "description": "Topic to test subscriptions.",
    "schema": {
      "type": "object",
      "properties": {
        "foo": {
          "type": "string"
        }
      },
      "identity": 1
    },
    "subscriptions": [
      {
        "type": "lambda",
        "logRetention": 14,
        "timeout": 15,
        "handler": {
          "name": "testHandler",
          "file": "test/input/output-subscriptions.ts"
        }
      },
      {
        "type": "queue",
        "service": "TestQueue"
      },
      {
        "type": "lambda",
        "memory": 128,
        "handler": {
          "name": "testHandler",
          "file": "test/input/output-subscriptions.ts"
        },
        "variables": {
          "TEST_VAR": "test-literal-value"
        }
      },
      {
        "type": "queue",
        "service": "TestQueue"
      }
    ]
  },
  "TestQueue": {
    "type": "@ez4/queue",
    "name": "TestQueue",
    "context": {},
    "variables": {},
    "services": {},
    "subscriptions": [],
    "schema": {
      "identity": 1,
      "properties": {
        "foo": {
          "type": "string"
        }
      },
      "type": "object"
    }
  }
}